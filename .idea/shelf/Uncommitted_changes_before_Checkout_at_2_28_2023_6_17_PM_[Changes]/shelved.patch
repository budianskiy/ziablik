<+>{% extends 'base.html' %}\r\n\r\n{% block meta_tags %}\r\n    {% if article.get_meta_title %}\r\n        {% include '_meta_tags.html' with meta_title=article.get_meta_title meta_discription=article.meta_description meta_keyword=article.meta_keywords %}\r\n    {% endif %}\r\n\r\n{% endblock %}\r\n\r\n{% block breadcrumbs %}\r\n    {% include '_breadcrumbs.html' %}\r\n{% endblock %}\r\n\r\n{% block content %}\r\n\r\n    <h2>{{ article.title }}</h2>\r\n    {% if article.image %}\r\n        <img class=\"img_thumbnail\" src=\"{{ article.image.url }}\" alt=\"\">\r\n    {% endif %}\r\n\r\n    <p>{{ article.text }}</p>\r\n\r\n    <p>\r\n        {% for tag in article.tags.all %}\r\n            <a href=\"{% url 'tag_search_view' tag_id=tag.id %}\">#{{ tag.name }}</a>&nbsp\r\n        {% endfor %}\r\n\r\n    </p>\r\n\r\n    <p>Автор статьи: {{ author }}</p>\r\n\r\n\r\n    <p style=\"text-align: right\">\r\n        Дата: <b>{{ article.publish_date }}</b>\r\n        Категория: <b>{{ category.name }}</b>\r\n    </p>\r\n\r\n    {% if article.user %}\r\n        <div class=\"text-end\">\r\n            <div class=\"author\">\r\n                {% if article.user.image %}\r\n                    <span class=\"avatar\">\r\n                        <img src=\"{{ article.user.image_thumbnail.url }}\" alt=\"\">\r\n                    </span>\r\n                {% endif %}\r\n                {% if article.user.first_name %}\r\n                    <span class=\"author-name\">{{ article.user.first_name }} {{ article.user.last_name }}</span>\r\n                {% else %}\r\n                    <span class=\"author-name\">{{ article.user.username }}</span>\r\n                {% endif %}\r\n            </div>\r\n        </div>\r\n    {% endif %}\r\n\r\n\r\n    <div class=\"row\">\r\n        <div class=\"col-sm-12 col-md-3\"></div>\r\n        <div class=\"col-sm-12 col-md-6\">\r\n            <h3 class=\"my-header\">Добавить коментарий</h3>\r\n            {% if error %}\r\n                <div class=\"alert alert-danger\" role=\"alert\">\r\n                    {{ error }}\r\n                </div>\r\n            {% endif %}\r\n            <form method=\"post\" enctype=\"multipart/form-data\">\r\n                {% csrf_token %}\r\n\r\n                {% if request.user.is_authenticated %}\r\n\r\n                    <div class=\"mb-3\">\r\n                        <label for=\"body\" class=\"form-label\">Текст коментария</label>\r\n                        <textarea name=\"body\" id=\"body\" cols=\"30\" rows=\"6\" class=\"form-control\"></textarea>\r\n                    </div>\r\n\r\n                {% else %}\r\n\r\n                    <div class=\"mb-3\">\r\n                        <label for=\"email\" class=\"form-label\">E-mail</label>\r\n                        <input type=\"email\" class=\"form-control\" id=\"email\" name=\"email\" required>\r\n                    </div>\r\n\r\n                    <div class=\"mb-3\">\r\n                        <label for=\"body\" class=\"form-label\">Текст коментария</label>\r\n                        <textarea name=\"body\" id=\"body\" cols=\"30\" rows=\"6\" class=\"form-control\"></textarea>\r\n                    </div>\r\n\r\n                {% endif %}\r\n            <button type=\"submit\" class=\"btn btn-primary\">Отправить</button>\r\n            </form>\r\n        </div>\r\n    </div>\r\n\r\n\r\n{% endblock %}
